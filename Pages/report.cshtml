@page
@model SAM2020.Pages.reportModel
@{
    ViewData["Title"] = "report";

    Reports reports = new Reports();
}

<h1>Report</h1>

<table class="table table-dark">
    <thead>
        <tr>
            <th>Total number of submitted papers</th>
            <th>@reports.getTotalSubmittedPapers()</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th>Total number of full reviewed papers</th>
            <th>@reports.getTotalReviewedPapers()</th>
        </tr>
        <tr>
            <th>Total number of not full reviewed papers</th>
            <th>@reports.getTotalNotReviewedPapers()</th>
        </tr>

        <tr>
            <th>Total number of papers has al lest one review</th>
            <th>@reports.getTotalAlLestOneReviewedPapers()</th>
        </tr>

    </tbody>
</table>
